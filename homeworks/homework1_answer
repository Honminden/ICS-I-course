1)
0011 1111 1111 :: 1777 :: 1023 :: 0x03FF
0010 0100 1001 :: 1111 ::  585 :: 0x0249
0100 0101 0111 :: 2127 :: 1111 :: 0x0457
0001 0001 0001 :: 0421 ::  273 :: 0x1111 (x)

2)
0x111(0001 0001 0001) + (1110 0010 1110 = 7056) = 1111 0011 1111
(0010 0100 0000 = 1100) * (10110000) = 0x18c0(0001 1000 1100 0000) (x)

3)
a = 0x22
b = 0xD2
c = 0x104 = (overflow) 0x04 (x)

4)
A = 0xB3 = 1011 0011
B = 0x2C = 0010 1100
A) A & B = 0010 0000 = 0x20
B) A && B = 0X20 (x)
C) A | B = 1011 1111 = 0xBF
D) A || B = 0xBF (x)
E) A ^ B = 1001 1111 = 0x9F
F) ~A | ~B = ~(A & B) = 0xDF

5)
0xA4 = 1010 0100 :: 1001 0000 = 0x90 :: 0001 0100 = 0x14 :: 1111 1010 = 0xFA
0xCE = 1100 1110 :: 0011 1000 = 0x38 :: 0001 1001 = 0x19 :: 1111 1100 = 0XFC
0xB9 = 1011 1001 :: 1110 0100 = 0xE4 :: 0001 0111 = 0x17 :: 1111 1011 = 0XFB
0x0E = 0000 1110 :: 0011 1000 = 0x38 :: 0000 0001 = 0x01 :: 1111 0000 = 0XF0 (x)
